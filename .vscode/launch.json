  "version": "0.2.0",
  "configurations": [
    {
      "name": "CollectStatic",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/vegitable/manage.py",
      "args": ["collectstatic", "--noinput"],
      "django": true,
      "justMyCode": true
    },

    {
      "name": "Django Run",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/vegitable/manage.py",
      "args": ["runserver", "0.0.0.0:8000"],
      "django": true,
      "justMyCode": true
    }
  ]
}
